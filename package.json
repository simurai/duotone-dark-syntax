{
  "name": "duotone-dark-syntax",
  "theme": "syntax",
  "version": "2.1.0",
  "description": "A dark DuoTone syntax theme",
  "keywords": [
    "dark",
    "purple",
    "gold",
    "duotone",
    "syntax"
  ],
  "repository": "https://github.com/simurai/duotone-dark-syntax",
  "license": "MIT",
  "main": "lib/duotone",
  "engines": {
    "atom": ">=1.13.0 <2.0.0"
  },
  "dependencies": {
    "duotone-syntax": "^2.1.0",
    "chroma-js": "1.1.1"
  },
  "configSchema": {
    "color": {
      "title": "Custom Colors",
      "type": "object",
      "properties": {
        "uno": {
          "title": "Uno",
          "description": "Customize the main color",
          "order": 1,
          "type": "color",
          "default": "#a59ccc"
        },
        "duo": {
          "title": "Duo",
          "description": "Customize the accent color",
          "order": 2,
          "type": "color",
          "default": "#ffe685"
        }
      }
    }
  }
}
